{
  "name": "dc-static-blog-nextjs",
  "version": "1.2.0",
  "description": "Amplience Dynamic Content Static Site with NextJS",
  "scripts": {
    "dev": "next",
    "precommit": "npm run lint",
    "commit": "npx git-cz",
    "lint": "prettier --check \"**/*.ts\" && eslint \"**/*.ts\"",
    "pretest": "npm run lint",
    "test": "jest --silent",
    "clean": "rimraf .next/* out/*",
    "prebuild": "npm run clean",
    "build": "next build",
    "preexport": "npm run build",
    "export": "next export",
    "prestart": "npm run export",
    "start": "serve",
    "fix": "prettier --write '**/*.ts'",
    "prerelease": "npm run test",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amplience/dc-static-blog-nextjs.git"
  },
  "author": "Amplience",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/amplience/dc-static-blog-nextjs/issues"
  },
  "homepage": "https://github.com/amplience/dc-static-blog-nextjs#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        100
      ],
      "type-case": [
        1,
        "always",
        "lower-case"
      ],
      "subject-case": [
        1,
        "always",
        "lower-case"
      ]
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/debounce": "^1.2.0",
    "@types/enzyme": "^3.10.4",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "@types/promise.allsettled": "^1.0.2",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-syntax-highlighter": "^10.2.1",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "async-wait-until": "^1.2.4",
    "babel-jest": "^24.9.0",
    "commitizen": "^4.0.3",
    "css-loader": "^3.5.2",
    "cz-conventional-changelog": "^3.0.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lodash.flow": "^3.5.0",
    "next-manifest": "^2.0.1",
    "next-offline": "^4.0.6",
    "next-seo": "^2.2.1",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "serve": "^11.3.0",
    "standard-version": "^7.0.1",
    "style-loader": "^1.1.4",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@types/react-instantsearch-dom": "^5.2.6",
    "algoliasearch": "^3.35.1",
    "dc-delivery-sdk-js": "^0.3.0",
    "debounce": "^1.2.0",
    "instantsearch.css": "^7.4.2",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.1.7",
    "promise.allsettled": "^1.0.2",
    "react": "^16.12.0",
    "react-cookie-consent": "^2.4.1",
    "react-dom": "^16.12.0",
    "react-instantsearch-dom": "^6.1.0",
    "react-lazy-load-image-component": "^1.4.0",
    "react-markdown": "^4.3.1",
    "react-syntax-highlighter": "^11.0.2"
  }
}
